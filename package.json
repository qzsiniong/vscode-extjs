{
	"name": "vscode-extjs",
	"displayName": "vscode-extjs",
	"description": "",
	"version": "0.0.8",
	"engines": {
		"vscode": "^1.44.0"
	},
	"publisher": "qzsiniong",
	"repository": {
		"type": "git",
		"url": "https://github.com/qzsiniong/vscode-extjs.git"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"workspaceContains:extjs.conf.json"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands":[
			{
				"command": "vscode-extjs:ensure-require",
				"title": "vscode-extjs:ensure-require"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@babel/parser": "^7.9.4",
		"@babel/types": "^7.9.5",
		"@types/glob": "^7.1.1",
		"@types/json5": "0.0.30",
		"@types/mocha": "^7.0.2",
		"@types/node": "^13.11.0",
		"@types/vscode": "^1.44.0",
		"@typescript-eslint/eslint-plugin": "^2.26.0",
		"@typescript-eslint/parser": "^2.26.0",
		"eslint": "^6.8.0",
		"glob": "^7.1.6",
		"mocha": "^7.1.1",
		"typescript": "^3.8.3",
		"vscode-test": "^1.3.0"
	},
	"dependencies": {
		"@babel/traverse": "^7.9.5",
		"@types/babel__traverse": "^7.0.10",
		"json5": "^2.1.3"
	}
}
